extends ./_common/_layout.jade
include ./_common/_mixin.jade
block title
	title Online shop
	- var history =  {'Главная': '#','Каталог':'#'}
	- var currentPage = 'Мобильные телефоны'
block content
	.content
		section.content-header
			.price-filter
				form.price-filter__form(method="post" id="price-form" action="#")
					span.filter_span Сортировать по
					select.sort-select(type="select" name="select__item" id="select__item")
						option.select__item(form="price__form") по цене
						option.select__item(form="price__form") по популярности
			.view-options
				span.view-options__text Вид каталога
				ul.view__list
					li.view__item.active(data-mode="detailed-list")
						a.view__item__link.view__item__link_detailed-list Detailed list
					li.view__item(data-mode="thumbnails")
						a.view__item__link.view__item__link_thumbnails Thumbnails
